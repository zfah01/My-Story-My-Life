      },
      "bundleIdentifier": "com.zfah09.MyStoryMyLifePassport"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zfah09.MyStoryMyLifePassport"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1f214b15-19f6-44f1-a83c-2b96cbf3dcc6"
      }
    }
  }
}
